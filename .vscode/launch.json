{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Run ddd-java",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "mainClass": "sample.Application",
            "projectName": "ddd-java",
            "args": "-Xmx128m",
            "env": {
                "SHOW_SQL": "true",
                "DDL_AUTO": "create-drop",
                "MAIL_ENABLED": "false",
                "MANAGEMENT_EXPOSURE": "*",
            },
        }
    ]
}